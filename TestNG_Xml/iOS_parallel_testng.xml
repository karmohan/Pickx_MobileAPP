<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="iOS Parallel Suite" thread-count="10" parallel = "tests">
<listeners>
		<listener class-name="utils.TestListener" />
<!-- <listener class-name="retryOnFailure.RetryListenerClass"/> -->
	</listeners>
	<test name="iOS run - Phone FR">
		 <parameter name="device" value="iPhone 12" />
		 <parameter name="language" value="FR" />
		<classes>
			<class name="testrunner.iOS_Testrunner_parallel" />
		</classes>
	</test>
	
	 <test name="iOS run - Phone EN">
		 <parameter name="device" value="iPhone 12" />
		 <parameter name="language" value="EN" />
		<classes>
			<class name="testrunner.iOS_Testrunner_parallel" />
		</classes>
	</test>
	
	<test name="iOS run - Phone NL">
		 <parameter name="device" value="iPhone 12" />
		 <parameter name="language" value="NL" />
		<classes>
			<class name="testrunner.iOS_Testrunner_parallel" />
		</classes>
	</test>
